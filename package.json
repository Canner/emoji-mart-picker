{
  "name": "emoji-mart-picker",
  "version": "0.1.9",
  "description": "emoji picker based on emoji-mart",
  "homepage": "",
  "author": {
    "name": "chilijung",
    "email": "chilijung@gmail.com",
    "url": ""
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "emoji-mart",
    "emoji",
    "picker"
  ],
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-runtime": "^6.11.6",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.14.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^5.0.1",
    "precommit-hook-eslint": "^3.0.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^16.2.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "scripts": {
    "lint": "eslint src test docs",
    "clean": "rimraf lib dist",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "npm run build:commonjs && cp ./src/*.css ./lib",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "check:src": "npm run lint",
    "test": "cross-env BABEL_ENV=test karma start --single-run",
    "test:watch": "cross-env BABEL_ENV=test karma start",
    "start": "node devServer.js",
    "build:docs": "cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "postpublish": "npm run build:docs",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": "Canner/emoji-mart-picker",
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@canner/emoji-mart": "^0.4.5",
    "rc-trigger": "^2.3.4"
  }
}
